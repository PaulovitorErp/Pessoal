Static Function PesqSra()
	Local nContador := 1

	If(SELECT("XQRY") > 0)
		XQRY->(DBCLOSEAREA())
	Endif

	aSize(aWBrowse1,0)

	cQuery:= " SELECT * FROM  "
	cQuery+=  Retsqlname("SRA")+" SRA  "
	cQuery+= " WHERE SRA.D_E_L_E_T_ = '  ' "
	cQuery+= "  AND SRA.RA_FILIAL  BETWEEN '"+Alltrim(cGet1)+"' AND '"+Alltrim(cGet2)+"' "
	if !Empty(cGet3) .AND. !EMPTY(cGet4)
		cQuery+= "  AND SRA.RA_MAT 	   BETWEEN '"+Alltrim(cGet3)+"' AND '"+Alltrim(cGet4)+"' "
	EndIf
	if !Empty(cGet5) .AND. !EMPTY(cGet6)
		cQuery+= "  AND SRA.RA_CC  	   BETWEEN '"+Alltrim(cGet5)+"' AND '"+Alltrim(cGet6)+"' "
	EndIf

	//cQuery+= "  AND SRA.RA_SITFOLH <> 'D' "   //Funcionario Demitido

	cQuery+= "ORDER BY SRA.RA_FILIAL,SRA.RA_MAT,SRA.RA_CC	"

	tcquery cQuery new alias "XQRY"

	While !XQRY->(eof())

		IncProc(" Funcionario .: "+XQRY->RA_MAT+' - '+XQRY->RA_NOME)


		Aadd(aWBrowse1,{.F.,;
		XQRY->RA_FILIAL,; 									  //FILIAL
		XQRY->RA_MAT,;										  //MATRICULA
		XQRY->RA_CC,;             						  //NOME
		PADR(XQRY->RA_NOME,200,' '),;              							  //CENTRO DE CUSTO
		STOD(XQRY->RA_ADMISSA)})

		nContador++


		XQRY->(dbskip())
	Enddo

	if nContador = 0
		Aadd(aWBrowse1,{.F.,'','','','',''})
	EndIf

Return()


