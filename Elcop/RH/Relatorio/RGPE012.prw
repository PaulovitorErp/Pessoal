Static Function AutDesc(cFileName, cDate)
	Local nHandle := FCREATE(cFileName,0)
	Local cContent := ""

	if nHandle > 0

	

	cContent := '<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">'+CRLF
	cContent += '<html xmlns="http://www.w3.org/1999/xhtml">'+CRLF
	cContent += '<head>'+CRLF
	cContent += '	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />'+CRLF
	cContent += '	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">'+CRLF
	cContent += '</head>'+CRLF
	cContent += '<body>'+CRLF
	cContent += '	<div style="width:900px; margin: 0 auto;">'+CRLF
	cContent += '		<p style="text-align:center; margin-top:10px; padding:0;"><img src="https://i.ibb.co/D91LPcS/logo.png" style="width:250px;"></p><br/>'+CRLF
	cContent += '	</div>'+CRLF
	cContent += '	<p style="font-weight: bolder; font-size: 14px; text-align: center; color: blue;">AUTORIZACAO DE DESCONTO EM FOLHA DE PAGAMENTO</p>'+CRLF
	cContent += '	<p style="font-size: 12px; text-align: left;">Pelo presente instrumento, eu <b>'+Alltrim(SRA->RA_NOME)+'</b>, brasileiro(a), inscrito(a) no CPF sob o nº, <b>'+Alltrim(SRA->RA_CIC)+'</b>, e no RG nº, <b>'+Alltrim(SRA->RA_RG)+'</b>, residente e domiciliado(a) à <b>'+Alltrim(SRA->RA_ENDEREC)+'</b>, autorizo a ELCOP ENGENHARIA LTDA, a efetuar o desconto em meu salario através da folha de pagamento, qualquer dano material por mim causado, inclusive multas de transito, multas por descumprimento de regras impostas por clientes, danos por nao devolucao, extravio ou mau uso de qualquer material da empresa.</p>'+CRLF
	cContent += '	<p style="font-size: 12px; text-align: left; color: red;"><b>'+ALLTRIM(SM0->M0_CIDENT)+'</b>, <b>'+Alltrim(cDate)+'</b>.</p>'+CRLF
	cContent += '</body>'+CRLF
	cContent += '</html>'+CRLF

	fwrite(nHandle, cContent)
	fClose(nHandle)
	EndIf
Return
