dbSelectArea( "SX2" )
SX2->( dbSetOrder( 1 ) )
SX2->( dbGoTop() )
cPath := SX2->X2_PATH
cPath := IIf( Right( AllTrim( cPath ), 1 ) <> "\", PadR( AllTrim( cPath ) + "\", Len( cPath ) ), cPath )
cEmpr := Substr( SX2->X2_ARQUIVO, 4 )

//
// Tabela U03
//
aAdd( aSX2, { ;
	'U03'																	, ; //X2_CHAVE
	cPath																	, ; //X2_PATH
	'U03'+cEmpr																, ; //X2_ARQUIVO
	'CABECALHO ALCADA'														, ; //X2_NOME
	'CABECALHO ALCADA'														, ; //X2_NOMESPA
	'CABECALHO ALCADA'														, ; //X2_NOMEENG
