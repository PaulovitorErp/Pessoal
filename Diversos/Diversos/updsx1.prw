	'C'																		, ; //X2_MODOUN
	0																		} ) //X2_MODULO

//
// Tabela UH8
//
aAdd( aSX2, { ;
	'UH8'																	, ; //X2_CHAVE
	cPath																	, ; //X2_PATH
	'UH8'+cEmpr																, ; //X2_ARQUIVO
	'SERVICOS PRESTADOS FORNECEDOR'											, ; //X2_NOME
	'SERVICOS PRESTADOS FORNECEDOR'											, ; //X2_NOMESPA
	'SERVICOS PRESTADOS FORNECEDOR'											, ; //X2_NOMEENG
	'E'																		, ; //X2_MODO
	''																		, ; //X2_TTS
	''																		, ; //X2_ROTINA
	''																		, ; //X2_PYME
	''																		, ; //X2_UNICO
	''																		, ; //X2_DISPLAY
	''																		, ; //X2_SYSOBJ
	''																		, ; //X2_USROBJ
	''																		, ; //X2_POSLGT
	''																		, ; //X2_CLOB
	''																		, ; //X2_AUTREC
