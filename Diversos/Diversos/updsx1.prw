	cPath																	, ; //X2_PATH
	'U92'+cEmpr																, ; //X2_ARQUIVO
	'TABELA DE RECADOS'														, ; //X2_NOME
	'TABELA DE RECADOS'														, ; //X2_NOMESPA
	'TABELA DE RECADOS'														, ; //X2_NOMEENG
	'C'																		, ; //X2_MODO
	''																		, ; //X2_TTS
	''																		, ; //X2_ROTINA
	''																		, ; //X2_PYME
	''																		, ; //X2_UNICO
	''																		, ; //X2_DISPLAY
	''																		, ; //X2_SYSOBJ
	''																		, ; //X2_USROBJ
	''																		, ; //X2_POSLGT
	''																		, ; //X2_CLOB
	''																		, ; //X2_AUTREC
	'C'																		, ; //X2_MODOEMP
	'C'																		, ; //X2_MODOUN
	0																		} ) //X2_MODULO

//
// Tabela U98
//
aAdd( aSX2, { ;
	'U98'																	, ; //X2_CHAVE
	cPath																	, ; //X2_PATH
	'U98'+cEmpr																, ; //X2_ARQUIVO
	'ADM. FIN. X LAYOUT (CAB)'												, ; //X2_NOME
	'ADM. FIN. X LAYOUT (CAB)'												, ; //X2_NOMESPA
	'ADM. FIN. X LAYOUT (CAB)'												, ; //X2_NOMEENG
	'C'																		, ; //X2_MODO
	''																		, ; //X2_TTS
	''																		, ; //X2_ROTINA
	''																		, ; //X2_PYME
	''																		, ; //X2_UNICO
	''																		, ; //X2_DISPLAY
	''																		, ; //X2_SYSOBJ
	''																		, ; //X2_USROBJ
	''																		, ; //X2_POSLGT
	''																		, ; //X2_CLOB
	''																		, ; //X2_AUTREC
