/*/{Protheus.doc} MNTA265Hea
Monta estrutura do aHeader.
@type function

@author Alexandre Santos
@since 23/02/2023

@param lOSCorret, boolean, Indica se a O.S. é do tipo corretiva.
/*/
//-----------------------------------------------------------------------------------
Function MNTA265Hea( lOSCorret )

	Local aNoFields := { 'TL_ORDEM', 'TL_PLANO'  , 'TL_SEQUENC', 'TL_SEQRELA', 'TL_NOMSEQ' , 'TL_NOMTREG', 'TL_CUSTO'   ,;
	    'TL_CUSTO2' , 'TL_CUSTO3'  , 'TL_CUSTO4' , 'TL_CUSTO5' , 'TL_CUSENT1', 'TL_CUSENT2', 'TL_CUSENT3', 'TL_LOCAPLI' ,;
		'TL_CUSENT4', 'TL_CUSENT5' , 'TL_OCORREN', 'TL_REPFIM' , 'TL_NUMSEQ' , 'TL_CODOBS' , 'TL_PCTHREX', 'TL_DTFIM'   ,;
		'TL_CONTROL', 'TL_ETAPA'   , 'TL_GARANTI', 'TL_NOMETAP', 'TL_NOMLOCA', 'TL_NUMLOTE', 'TL_LOTECTL', 'TL_HOFIM'   ,;
		'TL_LOCALIZ', 'TL_DTVALID' , 'TL_NUMSERI', 'TL_POSCONT', 'TL_POSCON2', 'TL_PERMDOE', "TL_NOMTAR" , "TL_OBSERVA" }

	If lOSCorret .And. !NGUSATARPAD()

		aAdd( aNoFields, 'TL_TAREFA' )

	EndIf

	aHeaIns := CabecGetD( 'STL', aNoFields, 2 )

	/*----------------------------------+
	| Inclui campo reservado ao aHeader |
	+----------------------------------*/
	ADHeadRec( 'STL', aHeaIns )

Return

