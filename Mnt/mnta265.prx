/*/{Protheus.doc} fAtuDtaOS
Atualiza as datas da O.S. conforme os insumos.
@type function

@author Alexandre Santos
@since 23/02/2023

@param cOrdem, string, Ordem de Serviço.
@param cPlano, string, Plano de Manutenção.
/*/
//-----------------------------------------------------------------------------------
Static Function fAtuDtaOS( cOrdem, cPlano )

	Local aBind   := {}
	Local cAlsDts := GetNextAlias()

	dbSelectArea( 'STJ' )
	dbSetOrder( 1 )
	msSeek( FWxFilial( 'STJ' ) + cOrdem + cPlano )

	/*----------------------------------------+
	| Verifica a existência da query em cache |
	+----------------------------------------*/
	If Empty( cQryDtInsu )

		cQryDtInsu :=	"SELECT "
		cQryDtInsu += 		"MIN( TEMP.TL_DTINICI || TEMP.TL_HOINICI ) AS MINDTHR, "
		cQryDtInsu += 		"MAX( TEMP.TL_DTFIM   || TEMP.TL_HOFIM   ) AS MAXDTHR  "
		cQryDtInsu +=	"FROM "
		cQryDtInsu += 		oTmpTbl1:GetRealName() + " TEMP "
		cQryDtInsu += 	"WHERE "
		cQryDtInsu += 		"TEMP.TL_ORDEM   = ? AND "
		cQryDtInsu += 		"TEMP.TL_PLANO   = ? AND "
		cQryDtInsu += 		"TEMP.STATUS IN ( 0, 1, 2 )"
		
		cQryDtInsu := ChangeQuery( cQryDtInsu )

	EndIf

	aAdd( aBind, STJ->TJ_ORDEM )
	aAdd( aBind, STJ->TJ_PLANO )

	dbUseArea( .T., 'TOPCONN', TcGenQry2( , , cQryDtInsu, aBind ), cAlsDts, .T., .T. )

	If !Empty( (cAlsDts)->MINDTHR ) .And. !Empty( (cAlsDts)->MAXDTHR )

		If (cAlsDts)->MINDTHR != DToS( STJ->TJ_DTMPINI ) + STJ->TJ_HOMPINI

			/*--------------------------------------------------------+
			| Atualizando data e hora inicio da O.S. conforme insumos |
			+--------------------------------------------------------*/
			NGUpdDtOrd( STJ->TJ_ORDEM, STJ->TJ_PLANO, 1, { SubStr( (cAlsDts)->MINDTHR, 1, 8 ),;
				SubStr( (cAlsDts)->MINDTHR, 9 ) } )

		EndIf

		If (cAlsDts)->MAXDTHR != DToS( STJ->TJ_DTMPFIM ) + STJ->TJ_HOMPFIM

			/*-----------------------------------------------------+
			| Atualizando data e hora fim da O.S. conforme insumos |
			+-----------------------------------------------------*/
			NGUpdDtOrd( STJ->TJ_ORDEM, STJ->TJ_PLANO, 2, { SubStr( (cAlsDts)->MAXDTHR, 1, 8 ),;
				SubStr( (cAlsDts)->MAXDTHR, 9 ) } )

		EndIf

	Else

		/*---------------------------------------------+
		| Atualizando data e hora inicio e fim da O.S. |
		+---------------------------------------------*/
		NGUpdDtOrd( STJ->TJ_ORDEM, STJ->TJ_PLANO, 0, { DToS( (cTRBA490)->TJ_DATANOV ), STJ->TJ_HOMPINI } )

	EndIf

	(cAlsDts)->( dbCloseArea() )

	/*----------------------------+
	| Desaloca consumo de memória |
	+----------------------------*/
	FWFreeArray( aBind )
	
Return
