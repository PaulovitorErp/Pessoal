//------------------------------------------------------------------------------
/*/{Protheus.doc} MNTA770
Cadastro de Infracao de Transito

@author NG Informática Ltda.
@since 01/01/2015
/*/
//------------------------------------------------------------------------------
Function MNTA770()
	
	Local oBrowse
	
	Private aRotina	  := {}
	Private cCadastro := ''
	
	If MNTAmIIn( 95 )

		aRotina	  := MenuDef()
		cCadastro := NGSX2NOME("TSH") // "Cadastro de Infracao de Transito"

		oBrowse := FWMBrowse():New()
		oBrowse:SetAlias( "TSH" )
		oBrowse:SetDescription( cCadastro )
		oBrowse:Activate()

	EndIf
	
Return .T.

//------------------------------------------------------------------------------
/*/{Protheus.doc} MenuDef
Interface da rotina

@author NG Informática Ltda.
@since 01/01/2015
/*/
//------------------------------------------------------------------------------
Static Function MenuDef()

	// Inicializa MenuDef com todas as opções
	Local aRotina := {}
	
	ADD OPTION aRotina Title STR0002 Action 'VIEWDEF.MNTA770' OPERATION 2 ACCESS 0 // "Visualizar"
	ADD OPTION aRotina Title STR0003 Action 'VIEWDEF.MNTA770' OPERATION 3 ACCESS 0 // "Incluir"
	ADD OPTION aRotina Title STR0004 Action 'VIEWDEF.MNTA770' OPERATION 4 ACCESS 0 // "Alterar"
	ADD OPTION aRotina Title STR0005 Action 'VIEWDEF.MNTA770' OPERATION 5 ACCESS 0 // "Excluir"
	ADD OPTION aRotina Title STR0013 Action 'VIEWDEF.MNTA770' OPERATION 8 ACCESS 0 // "Imprimir"
	ADD OPTION aRotina Title STR0012 Action 'VIEWDEF.MNTA770' OPERATION 9 ACCESS 0 // "Copiar"
	
	If cPaisLoc == 'BRA'
		ADD OPTION aRotina Title STR0011 Action 'Processa( { || MNT770CAR() }, "' + STR0006 + '", "' + STR0007 + '" )' OPERATION 4 ACCESS 0
	EndIf

Return aRotina

//------------------------------------------------------------------------------
/*/{Protheus.doc} ModelDef
Regras de Modelagem da gravacao

@author NG Informática Ltda.
@since 01/01/2015
/*/
//------------------------------------------------------------------------------
Static Function ModelDef()

	Local oModel
	Local oStructTRU := FWFormStruct(1,"TSH")

	// Cria o objeto do Modelo de Dados
	oModel := MPFormModel():New("MNTA770")

	// Adiciona ao modelo uma estrutura de formulário de edição por campo
	oModel:AddFields("MNTA770_TSH", Nil, oStructTRU)

	oModel:SetDescription( NGSX2NOME("TSH") )

Return oModel

//------------------------------------------------------------------------------
/*/{Protheus.doc} ViewDef
Regras de Interface com o Usuario

@author NG Informática Ltda.
@since 01/01/2015
/*/
//------------------------------------------------------------------------------
Static Function ViewDef()

	Local oModel := FWLoadModel("MNTA770")
	Local oView  := FWFormView():New()

	// Objeto do model a se associar a view.
	oView:SetModel(oModel)

	//Adiciona no nosso View um controle do tipo FormFields(antiga enchoice)
	oView:AddField( "MNTA770_TSH" , FWFormStruct(2,"TSH") )	

	// Criar um "box" horizontal para receber algum elemento da view
	oView:CreateHorizontalBox( "MASTER" , 100 )

	// Associa um View a um box
	oView:SetOwnerView( "MNTA770_TSH" , "MASTER" )
	
	// Inclusão de itens no Ações Relacionadas de acordo com o NGRightClick
	NGMVCUserBtn( oView )

Return oView

//-------------------------------------------------------------------
/*/{Protheus.doc} MNT770CAR
Função responsável pelo processamento da carga de dados na tabela TSH

@type   Function

@author Eduardo Mussi
@since  04/12/2023
/*/
//-------------------------------------------------------------------
Function MNT770CAR()

	Local aInfra  := {}
	Local cFilTSH := FwxFilial( 'TSH' )
	Local nInfra  := 0
	Local nTamInf := 0

	If MsgYesNo( STR0009, STR0008 ) // 'Deseja realmente importar todas as infrações do CTB?' // Atenção
		
		aInfra  := fInfracoes()
		nTamInf := Len( aInfra )
		
		ProcRegua( nTamInf )
		
		dbSelectArea( 'TSH' )
		dbSetOrder( 1 )
		For nInfra := 1 To nTamInf
			
			IncProc( STR0010 + cValToChar( nInfra ) + '/'  + cValToChar( nTamInf ) ) // 'Importando infrações '

			If !MsSeek( cFilTSH + aInfra[ nInfra, 1 ] )
				RecLock( 'TSH', .T. )
				TSH->TSH_FILIAL := cFilTSH
				TSH->TSH_CODINF := aInfra[ nInfra, 1 ]
				TSH->TSH_ARTIGO := aInfra[ nInfra, 2 ]
				TSH->TSH_DESART := aInfra[ nInfra, 3 ]
				TSH->TSH_VALOR  := aInfra[ nInfra, 4 ]
				TSH->TSH_PONTOS := aInfra[ nInfra, 5 ]
				TSH->TSH_RESPON := aInfra[ nInfra, 6 ]
				TSH->TSH_FLGTPM := '1'
				TSH->( MsUnLock() )
			EndIf
			
		Next nInfra

		FwFreeArray( aInfra )
	
	EndIf
	
Return 

